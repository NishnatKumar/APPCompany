    },
    "android": {
      "package": "com.depixed.DocUpload"
    
    },
    "androidStatusBar": {
       
      "barStyle": "light-content",
  
    
      "backgroundColor": "#F25E00"
    },
    "notification": {
    
      "icon":"./assets/icon1.png",
  
     
      "color":"#F25E00",
  
     
